    "keywords": [
        "rbac",
        "Permission"
    ],
    "require": {
        "tymon/jwt-auth": "1.0.1",
        "prettus/l5-repository": "2.7.0",
        "dingo/api": "^3.0.4"
    },
    "autoload": {
